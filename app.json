{
  "expo": {
    "name": "Problem to Hero",
    "slug": "qa-native-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ef4444"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.qa.app",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false
      }
    },
    "android": {
      "package": "com.qa.app",
      "adaptiveIcon": {
        "backgroundColor": "#ef4444"
      },
      "statusBar": {
        "backgroundColor": "#ffffff",
        "barStyle": "dark-content"
      },
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "bundler": "metro"
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "barStyle": "dark-content",
      "translucent": false
    },
    "extra": {
      "eas": {
        "projectId": "a26aa3e4-1181-430f-838b-a9b78c97a859"
      }
    }
  }
}
